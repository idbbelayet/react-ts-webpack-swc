{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": true
    },
    "target": "es2020",
    "transform": {
      "react": {
        "runtime": "automatic",
        "importSource": "react"
      }
    }
  },
  "module": {
    "type": "es6",
    "strict": true
  },
  "sourceMaps": true
}
